<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomToolbar">
        <attr name="getStringForBundle" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomDialog">
        <attr name="namePositiveButton" format="string" />
        <attr name="nameNegativeButton" format="string" />
        <attr name="editTextHintFirstField" format="string" />
        <attr name="editTextHintSecondField" format="string" />
        <attr name="howManyFieldsToDisplay" format="integer">
            <enum name="firstField" value="1"/>
            <enum name="secondField" value="2"/>
            <enum name="ThirdField" value="3"/>
            <enum name="allField" value="4"/>
        </attr>
    </declare-styleable>
</resources>